{
  "name": "Nosey-React",
  "private": true,
  "description": "Nosey-TV React SPA by dotstudioPRO",
  "author": "dotstudioPRO <support@dotstudiopro.com>",
  "version": "1.0.0",
  "homepage": "https://github.com/erikras/react-redux-universal-hot-example",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch",
    "test-local": "npm run single && npm run local && npm run parallel",
    "test-local-chrome": "node test/scripts/local.runner.js -c test/config/nightwatch-chrome.json",
    "test-chrome": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e chrome",
    "test-local-firefox": "node test/scripts/local.runner.js -c test/config/nightwatch-firefox.json",
    "test-firefox": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e firefox",
    "test-safari9": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e safari9",
    "test-local-safari": "node test/scripts/local.runner.js -c test/config/nightwatch-safari.json",
    "test-safari": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e safari",
    "test-iphone5": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e iPhone5",
    "test-ipad": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e iPad",
    "test-ipadmini": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e iPadMini",
    "browserstack": "node test/scripts/local.runner.js -c test/config/parallel_local.conf.js -e chrome,firefox,safari,iPhone5,iPad,iPadMini,samsungGalaxyS5,nexux7,kindleFire2,ie8OnWindows7,ie9OnWindows7,ie10OnWindows7,ie11OnWindows7"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
